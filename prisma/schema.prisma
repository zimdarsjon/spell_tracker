datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Spell {
  id String  @default(cuid()) @id
  name String?
  desc String?
  higher_level String?
  range String?
  material String?
  ritual Boolean?
  components String[]
  duration String?
  level Int?
  school String?
  concentration Boolean?
  casting_time String?
}
